/**
 * 判断是否已登录.
 * 
 * @param cbFun
 */
function isLogin(cbFun) {
	var url = "/SuperW/isLogin.action";
	$.getJSON(url, {
		"noCache" : Date()
	}, function(json) {
		if (json) {
			if (json.serviceResult == true) {
				localStoragePut("isLogin", json.resultParm.isLogin);
				cbFun();
			} else {
				alert(json.resultInfo);
			}
		} else {
			alert("数据库连接失败，请联系技术工程师");
		}
	});
}

